{
  "name": "where-2-compare",
  "author": "me@stevensoule.com",
  "license": "MIT",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "install": "run-p install:server install:app",
    "install:server": "cd server && npm install",
    "install:app": "cd app && npm install",
    "local": "run-p local:server local:app",
    "local:server": "cd server && npm run local",
    "local:app": "cd app && npm run serve",
    "postinstall": "if test \"$NODE_ENV\" = \"production\" ; then cd app && npm install && npm run build ; fi ",
    "build": "cd app && npm run build",
    "start": "cd server && npm run server"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.15",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "@vue/eslint-config-prettier": "^3.0.0-beta.15",
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^22.4.3",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.16",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "12.9.0"
  }
}
