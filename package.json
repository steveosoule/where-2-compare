{
  "name": "where-2-compare",
  "author": "me@stevensoule.com",
  "license": "MIT",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "install": "run-p install:server install:app",
    "install:server": "cd server && pnpm install",
    "install:app": "cd app && pnpm install",
    "local": "run-p local:server local:app",
    "local:server": "cd server && pnpm run local",
    "local:app": "cd app && pnpm run serve",
    "postinstall": "if test \"$NODE_ENV\" = \"production\" ; then cd app && pnpm install && pnpm run build ; fi ",
    "build": "cd app && pnpm run build",
    "start": "cd server && pnpm run server"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "pnpm": "^3.8.1"
  },
  "engines": {
    "node": "12.9.0"
  }
}
